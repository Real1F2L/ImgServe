{
    "Server": "ImgServe",
    "ServerURL": "example.com",
    "ServerVersion": "v1.1.0",

    "fileLocation": "/app/Files",
    "cacheLocation": "/app/Cache",
    "logLocation": "/app/Logs",

    "doClearCache": true,
    "clearCacheEveryHours": 24,
    "ignoreFiles": [
        "Dockerfile",
        ".gitignore",
        ".venv",
        ".pyCache",
        "__pycache__",
        ".git"
    ]
}